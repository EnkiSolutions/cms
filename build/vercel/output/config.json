{
    "version": 3,
    "routes": [
        {
            "src": "^/api/(.*)",
            "dest": "/api/$1",
            "continue": false
        },
        {
            "src": "^/(.*)\\.(js|css|json|webp|png|jpg|jpeg|gif|ico|svg)$",
            "dest": "/$1.$2"
        },
        {
            "src": "/.*",
            "dest": "/index.html",
            "continue": false
        }
    ]
}