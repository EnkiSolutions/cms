{
    "version": 3,
    "routes": [
        {
            "src": "^/static/(.*)",
            "headers": { "cache-control": "s-maxage=31536000, immutable" },
            "dest": "/static/$1"
        },
        {
            "src": "^/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|json)$",
            "dest": "/$1.$2"
        },
        {
            "src": "/.*",
            "dest": "/index.html",
            "continue": false
        }
    ]
}