{
    "version": 3,
    "routes": [
        {
            "handle": "rewrite",
            "src": "/contents/:match*",
            "dest": "/contents/:match*"
        },
        {
            "handle": "rewrite",
            "src": "/pages/:match*",
            "dest": "/pages/:match*"
        },
        {
            "handle": "rewrite",
            "src": "/:path*",
            "dest": "/index.html"
        }
    ]
}